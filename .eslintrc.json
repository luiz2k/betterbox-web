{
  "extends": ["next/core-web-vitals", "plugin:tailwindcss/recommended", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "singleQuote": true,
        "semi": true,
        "tabWidth": 2,
        "arrowParens": "always",
        "endOfLine": "auto"
      }
    ]
}
}
